{
  "version": 2,
  "buildCommand": "npm --prefix frontend run build",
  "outputDirectory": "frontend/build",
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.+)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

